container_commands:
  01_migrate_db:
    command: docker exec `docker ps -l -q -f 'status=running'` knex migrate:latest NODE_ENV=production
    leader_only: true
    ignoreErrors: false
  02_seed_db:
    command: docker exec `docker ps -l -q -f 'status=running'` knex seed:run node_ENV=production
    leader_only: true
    ignoreErrors: false